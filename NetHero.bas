Function zEncrypt(Password As String)
    Dim Something As String
    Dim TheCur As String
    Dim Asdf As String
    Dim Asdf2 As String
    Dim Asdf3 As String
    
    TheKey = Len(Password)
    For i = 0 To TheKey - 1
        TheCur = Mid(Password, i + 1, 1)
        Asdf = CharacterToNum(TheCur)
        Asdf = Asdf - i
        Asdf2 = NumToCharacter(Asdf)
        Asdf3 = Convert(Asdf2)
        SomeString = SomeString + Asdf3
    Next i
    zEncrypt = SomeString
End Function

